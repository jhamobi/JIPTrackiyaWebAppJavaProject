package com.jhamobi.trackiya.client;

public class VehicleDetail {
	private final String mVehicleNum;
	private final String mVehicleType;
	private final String mDriverName;
	private final String mDriverNum;
	private final String mOwnerName;
	private final String mOwnerNum;
	private final String mRateWithin1500Km;
	private final String mRateAbove1500Km;
	private final String mRateWithin12Hrs;
	private final String mRateAbove12Hrs;

	public VehicleDetail(String vehicleNum, String vehicleType, String driverName, String driverNum, String ownerName,
			String ownerNum, String rateWithin1500Km, String rateAbove1500Km, String rateWithin12Hrs,
			String rateAbove12Hrs) {
		this.mVehicleNum = vehicleNum;
		this.mVehicleType = vehicleType;
		this.mDriverName = driverName;
		this.mDriverNum = driverNum;
		this.mOwnerName = ownerName;
		this.mOwnerNum = ownerNum;
		this.mRateWithin1500Km = rateWithin1500Km;
		this.mRateAbove1500Km = rateAbove1500Km;
		this.mRateWithin12Hrs = rateWithin12Hrs;
		this.mRateAbove12Hrs = rateAbove12Hrs;

	}

	public String getVehicleNum() {
		return mVehicleNum;

	}

	public String getVehicleType() {
		return mVehicleType;

	}

	public String getDriverName() {
		return mDriverName;

	}

	public String getDriverNum() {
		return mDriverNum;

	}

	public String getOwnerName() {
		return mOwnerName;

	}

	public String getOwnerNum() {
		return mOwnerNum;

	}

	public String getRateWithin1500Km() {
		return mRateWithin1500Km;

	}

	public String getRateAbove1500Km() {
		return mRateAbove1500Km;

	}

	public String getRateWithin12Hrs() {
		return mRateWithin12Hrs;

	}

	public String getRateAbove12Hrs() {
		return mRateAbove12Hrs;

	}
}
