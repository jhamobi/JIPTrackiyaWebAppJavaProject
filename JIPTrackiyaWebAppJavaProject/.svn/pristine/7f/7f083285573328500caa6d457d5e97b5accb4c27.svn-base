package com.jhamobi.trackiya.client;

public class VehicleDetail {
	private final String mVehicleNum;
	private final String mVehicleType;
	private final String mVehicleRunningStatus;
	private final String mDriverName;
	private final String mDriverNum;
	private final String mOwnerName;
	private final String mOwnerNum;
	private final String mVehicleCurrLoc;
	private final String mVehicleTimeLapsed;


	public VehicleDetail(String vehicleNum, String vehicleType, String driverName,
			String driverNum, String ownerName, String ownerNum , String vehicleRunningStatus, String currLoc , String timeLapsed ) {
		this.mVehicleNum = vehicleNum;
		this.mVehicleType = vehicleType;
		this.mDriverName = driverName;
		this.mDriverNum = driverNum;
		this.mOwnerName = ownerName;
		this.mOwnerNum = ownerNum;
		this.mVehicleRunningStatus = vehicleRunningStatus;
		this.mVehicleCurrLoc = currLoc;
		this.mVehicleTimeLapsed = timeLapsed;

	}

	public String getVehicleNum() {
		return mVehicleNum;

	}

	public String getVehicleType() {
		return mVehicleType;

	}

	public String getVehicleRunningStatus() {
		return mVehicleRunningStatus;

	}

	public String getDriverName() {
		return mDriverName;

	}

	public String getDriverNum() {
		return mDriverNum;

	}
	
	public String getOwnerName() {
		return mOwnerName;

	}

	public String getOwnerNum() {
		return mOwnerNum;

	}
	
	public String getVehicleCurrLoc() {
		return mVehicleCurrLoc;

	}
	
	public String getVehicleTimeLapsed() {
		return mVehicleTimeLapsed;

	}
}
